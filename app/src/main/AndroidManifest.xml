<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.park24x7.incrediblesahibganj">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".network.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.WelcomeActivity"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustPan|stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LandingActivity"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity
            android:name=".touristattraction.TouristAttactionListActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.ActionBar"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity
            android:name=".touristattraction.GalleryActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.ActionBar"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity
            android:name=".atm.ATMActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.ActionBar"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity
            android:name=".touristattraction.TouristAttractionActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.ActionBar"
            android:windowSoftInputMode="adjustPan|stateHidden" />


    </application>

</manifest>